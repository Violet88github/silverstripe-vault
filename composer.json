{
    "name": "violet88/silverstripe-vault-module",
    "description": "A SilverStripe module for encrypting and decrypting data using the HashiCorp Vault API",
    "type": "silverstripe-vendormodule",
    "keywords": [
        "silverstripe",
        "CMS",
        "vault",
        "encryption",
        "decryption",
        "hashicorp"
    ],
    "license": "BSD-3-Clause",
    "require": {
        "silverstripe/framework": "^4.0",
        "silverstripe/admin": "^1.0",
        "php": ">=7.4"
    },
    "require-dev": {
        "phpunit/phpunit": ">=7.0",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "SilverStripeModule\\Skeleton\\": "src/",
            "SilverStripeModule\\Skeleton\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.x-dev"
        },
        "expose": [
            "client/dist",
            "client/src"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "allow-plugins": {
            "composer/installers": true,
            "silverstripe/vendor-plugin": true
        }
    }
}
